ldw r2, 3(r0)
ldw r7, 4(r0)
ldw r8, 5(r0)
ldw r9, 6(r0)
ldw r10 7(r0)
ldw r3, 8(r0)
addi r5, r0, 4
addi r6, r0, 12
addi r11, r0, 14
addi r12, r0, 10
LOOP: 	
ldw r4, 0(r2)
stw r5, 0(r3)
stw r0, 0(r7)
stw r0, 0(r8)
stw r0, 0(r9)
stw r0, 0(r10)
cmp r5, r4
beq LEDS
br LOOP		
LEDS:	
stw r6, 0(r3)
stw r6, 0(r7)
stw r11, 0(r8)
stw r12, 0(r9)
stw r6, 0(r10)
ldw r4, 0(r2)
cmp r5,r4
beq LEDS
br  LOOP
nop
